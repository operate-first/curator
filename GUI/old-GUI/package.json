{
  "name": "patternfly-seed",
  "version": "0.0.1",
  "description": "An open source build scaffolding utility for web apps.",
  "main": "server.js",
  "repository": "https://github.com/patternfly/patternfly-react-seed.git",
  "license": "MIT",
  "private": true,
  "homepage": "./",
  "scripts": {
    "prebuild": "npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config webpack.prod.js",
    "start": "node server.js",
    "start:dev": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/node": "^13.7.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.2",
    "css-loader": "^3.5.3",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.4",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.15.2",
    "react": "^16.11.0",
    "react-axe": "^3.4.1",
    "react-dom": "^16.11.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.2",
    "url-loader": "^3.0.0",
    "webpack-merge": "^4.1.4",
    "webpack": "^4.41.6",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.6.1",
    "@patternfly/react-icons": "^4.1.1",
    "@patternfly/react-styles": "^4.1.1",
    "@patternfly/react-table": "^2.28.50",
    "axios": "^0.21.1",
    "express": "^4.13.4",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "oauth2-client-js": "0.0.15",
    "react-router-dom": "^5.0.1",
    "react-router-last-location": "^2.0.1"
  }
}
